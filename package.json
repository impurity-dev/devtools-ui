{
    "name": "devtools-ui",
    "version": "0.0.0",
    "scripts": {
        "start": "npm run build && npm run start:storybook",
        "start:storybook": "start-storybook -p 6006",
        "test": "ng test",
        "test:ci": "npm test -- --sourceMap=false --browsers=ChromeHeadless --watch=false",
        "lint": "ng lint @impurity/devtools-ui",
        "lint:fix": "npm run lint -- --fix",
        "build": "npm run build:devtools-ui && npm run build:styles && npm run build:storybook",
        "build:devtools-ui": "ng build @impurity/devtools-ui",
        "build:storybook": "build-storybook -c .storybook -o dist/storybook -s dist/impurity/devtools-ui/css",
        "build:styles": "npm run build:styles:scss && npm run build:styles:css",
        "build:styles:scss": "sass projects/impurity/devtools-ui/src/scss/styles.scss dist/impurity/devtools-ui/scss/styles.scss --load-path=node_modules",
        "build:styles:css": "sass projects/impurity/devtools-ui/src/scss/styles.scss dist/impurity/devtools-ui/css/styles.css --load-path=node_modules"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^9.0.7",
        "@angular/cdk": "^9.1.3",
        "@angular/common": "^9.0.7",
        "@angular/compiler": "^9.0.7",
        "@angular/core": "^9.0.7",
        "@angular/forms": "^9.0.7",
        "@angular/material": "^9.1.3",
        "@angular/platform-browser": "^9.0.7",
        "@angular/platform-browser-dynamic": "^9.0.7",
        "@angular/router": "^9.0.7",
        "@storybook/addon-actions": "^5.3.17",
        "@storybook/addon-knobs": "^5.3.17",
        "@storybook/addon-links": "^5.3.17",
        "@storybook/addon-notes": "^5.3.17",
        "@storybook/addons": "^5.3.17",
        "@storybook/angular": "^5.3.17",
        "bootstrap": "^4.4.1",
        "rxjs": "~6.5.4",
        "tslib": "^1.11.1",
        "zone.js": "^0.10.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.900.7",
        "@angular-devkit/build-ng-packagr": "^0.900.7",
        "@angular/cli": "^9.0.7",
        "@angular/compiler-cli": "^9.0.7",
        "@angular/language-service": "^9.0.7",
        "@babel/core": "^7.9.0",
        "@types/jasmine": "^3.5.9",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "^12.12.30",
        "babel-loader": "^8.1.0",
        "codelyzer": "^5.1.2",
        "husky": "^4.2.3",
        "jasmine-core": "~3.5.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~4.3.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~2.1.0",
        "karma-jasmine": "~2.0.1",
        "karma-jasmine-html-reporter": "^1.4.2",
        "ng-packagr": "^9.0.3",
        "prettier": "^1.19.1",
        "pretty-quick": "^2.0.1",
        "protractor": "~5.4.3",
        "sass": "^1.26.3",
        "storybook-addon-themes": "^5.4.0",
        "ts-node": "~8.3.0",
        "tslint": "~5.18.0",
        "typescript": "~3.7.5"
    }
}
